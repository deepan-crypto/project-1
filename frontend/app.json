{
  "expo": {
    "name": "Thots",
    "slug": "thots",
    "version": "1.0.0",
    "android": {
      "package": "com.deepangokul.cryptoapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ican.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS"
      ],
      "notification": {
        "icon": "./assets/images/ican.png",
        "color": "#458FD0"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/images/ican.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/ican.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deepangokul.cryptoapp"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/ican.png",
          "color": "#458FD0"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "09787720-c92e-4247-943b-1dbe394d4a71"
      }
    }
  }
}