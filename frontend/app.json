{
  "expo": {
    "name": "crypto-app",
    "slug": "crypto-app",
    "version": "1.0.0",
    
    
    "android": {
      "package": "com.deepangokul.cryptoapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ican.png",
        "backgroundColor": "#ffffff"
      }
    },
   

    "orientation": "portrait",
    "icon": "./assets/images/ican.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "09787720-c92e-4247-943b-1dbe394d4a71"
      }
    }
  }
}